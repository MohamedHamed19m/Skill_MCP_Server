{
  "name": "skills",
  "version": "1.0.0",
  "description": "Skills extension: Lazy-load domain expertise, context, and capabilities. Tools for discovery/loading skills.",
  "mcpServers": {
    "skills-server": {
      "command": "uv run python ${extensionPath}${/}mcp_app${/}skills_server.py",
      "cwd": "${extensionPath}",
      "env": {
        "SKILLS_DIR": "${extensionPath}${/}skills"
      }
    }
  },
  "contextFileName": "GEMINI.md",
  "excludeTools": []
}
